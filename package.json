{
  "name": "spotify-analysis-tool",
  "version": "1.0.0",
  "private": true,
  "description": "Spotify Analysis Tool - Full Stack Application",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "node --loader ts-node/esm lib/server.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "node build.js",
    "build:backend": "tsc --project tsconfig.server.json",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc --project tsconfig.server.json --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "express-session": "^1.17.3",
    "cookie-parser": "^1.4.6",
    "@vercel/kv": "^1.0.1",
    "connect-redis": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "esbuild": "^0.19.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "autoprefixer": "^10.4.22",
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.17.10",
    "@types/cookie-parser": "^1.4.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  }
}
