{
  "name": "spotify-analysis-backend",
  "version": "1.0.0",
  "description": "Backend API for Spotify Analysis Tool",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "build:vercel": "tsc --project tsconfig.vercel.json",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "vercel-build": "echo 'Build complete'"
  },
  "keywords": ["spotify", "api", "analysis"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "express-session": "^1.17.3",
    "cookie-parser": "^1.4.6",
    "@vercel/kv": "^1.0.1",
    "connect-redis": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.17.10",
    "@types/cookie-parser": "^1.4.6",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  }
}
